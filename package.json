{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:analyze": "vite build --mode production --config vite.config.analyze.ts",
    "preview:prod": "vite preview --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "security:audit": "npm audit && npm audit fix",
    "test:security": "npm run security:audit && npm run lint && npm run type-check && npm run test:all",
    "deploy:check": "npm run test:security && npm run build:prod && npm run preview:prod",
    "deploy:prod": "npm run deploy:check && echo 'Production build ready for deployment'",
    "deploy:staging": "npm run build -- --mode staging && echo 'Staging build ready'",
    "health:check": "curl -f http://localhost:4173 || exit 1",
    "size:check": "npm run build:prod && npx bundlesize",
    "perf:audit": "npm run build:prod && npx lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html",
    "test": "jest",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --testPathPatterns=integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postbuild": "echo 'Build completed successfully. Ready for deploy.'",
    "prebuild": "npm run lint && npm run type-check",
    "postinstall": "npm run security:audit"
  },
  "dependencies": {
    "@sentry/react": "^10.12.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/dompurify": "^3.2.0",
    "dompurify": "^3.2.7",
    "framer-motion": "^12.23.13",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "cypress": "^15.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
